<% layout("/layouts/boilerplate")%>
<div class="row  row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
  <% for(let listings of allListings) { %>
  <a href="/listings/<%=listings._id%>" class="listing-link">
  <div class="card">
    <img src="<%=listings.image%>" class="card-img-top " alt="Error" style="height:20rem;">
    <div class="card-body">
     <b> <p class="card-text"><%=listings.title%><br></b> &#8377;<%=listings.price.toLocaleString("en-IN")%></p>
  </div>
</div>
</a>
<%}%>
</div>